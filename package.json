{
  "name": "not-deku",
  "description": "Render interfaces using pure functions and virtual DOM, kinda",
  "version": "0.0.5",
  "author": "Rico Sta. Cruz <rico@ricostacruz.com>",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "syntax-jsx",
      [
        "transform-react-jsx",
        {
          "pragma": "element"
        }
      ]
    ]
  },
  "bugs": {
    "url": "https://github.com/rstacruz/not-deku/issues"
  },
  "dependencies": {
    "virtual-dom": "2.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.3.17",
    "babel-plugin-syntax-jsx": "6.3.13",
    "babel-plugin-transform-react-jsx": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babelify": "7.2.0",
    "deku": "2.0.0-rc6",
    "hihat": "2.5.0",
    "jsdom": "7.2.2",
    "tap-dev-tool": "1.3.0",
    "tap-spec": "4.1.1",
    "tape": "4.4.0"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/rstacruz/not-deku#readme",
  "keywords": [
    "deku",
    "dom",
    "elm",
    "functional",
    "react",
    "redux",
    "virtual"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rstacruz/not-deku.git"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build",
    "test": "hihat test/index.js -- -t babelify --debug -p tap-dev-tool",
    "test:jsdom": "babel-node test/_jsdom.js | tap-spec",
    "watch": "babel -w src --out-dir lib"
  }
}
